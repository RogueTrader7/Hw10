{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/RogueTrader7/Homework10/main/normalisedtree.csv"
    },
    "width":"container",
    "mark": "bar",
    "encoding": {
      "x": {
        "field": "State",
        "type": "ordinal",
        "title": "Regions"
      },
      "y": {
        "field": "people",
        "type": "quantitative",
        "scale": {"type": "quantitative", "domain": [10, 100]}

      },
      "color": {
        "type": "nominal",
        "field": "health status",
        "scale": {
          "domain": [
            "Excellent",
            "Very good",
            "Excellent / Very good",
            "Good",
            "Fair",
            "Fair / Poor"
          ],
          "range": [
            "#d73027",
            "#fc8d59",
            "#ffffbf",
            "#e0f3f8",
            "#91bfdb",
            "#4575b4"
          ]
        },
        "title": "Health Status"        
      },
      "tooltip": [
        {"field": "State", "type": "nominal"},
        {"field": "people", "type": "quantitative", "format": ","},
        {"field": "health status", "type": "nominal", "format": ","}
        ]
       
    },
    "config": {}
  }